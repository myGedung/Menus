{
    "name": "mygedung/menus",
    "description": "Laravel Menu management.",
    "type": "package",
	"keywords": [
		"laravel",
		"menus",
		"navigation",
		"nav",
		"bootstrap"
	],
    "license": "MIT",
    "authors": [
        {
            "name": "Saidi Yusof",
            "email": "mygedungpro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.5",
        "illuminate/config": "~5.5",
        "illuminate/view": "~5.5",
        "laravelcollective/html": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.5",
        "phpro/grumphp": "~0.12.0",
        "friendsofphp/php-cs-fixer": "~2.7"
    },
	"autoload": {
		"psr-4": {
			"myGedung\\Menus\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"myGedung\\Menus\\Tests\\": "Tests"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"myGedung\\Menus\\MenusServiceProvider"
			],
			"aliases": {
				"Menu": "myGedung\\Menus\\Facades\\Menu"
			}
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
